<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Excluded">
        <Item>
          <IdeaActionID>MarkExcludeRoot</IdeaActionID>
          <ShortCut>'Alt+M'</ShortCut>
          <Description>Excluded</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'dev'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'dev'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Event Log">
        <Item>
          <IdeaActionID>ActivateEventLogToolWindow</IdeaActionID>
          <Description>Event Log</Description>
        </Item>
      </Statistic>
      <Statistic Action="Favorites">
        <Item>
          <IdeaActionID>ActivateFavoritesToolWindow</IdeaActionID>
          <ShortCut>'Alt+2'</ShortCut>
          <Description>Favorites</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="npm">
        <Item>
          <IdeaActionID>ActivatenpmToolWindow</IdeaActionID>
          <Description>npm</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>