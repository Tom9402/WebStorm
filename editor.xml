<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="ECMAScript 6" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="LESS" value="true" />
        <entry key="Stylus" value="false" />
        <entry key="TypeScript" value="false" />
        <entry key="XHTML" value="false" />
        <entry key="XML" value="false" />
      </map>
    </option>
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="500" restartIfHotSwapFailed="true" />
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>