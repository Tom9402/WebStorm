<keymap version="1" name=" My Keymap" parent="$default">
  <action id="CheckinFiles">
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="CheckinProject" />
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift alt delete" />
  </action>
  <action id="CloseAllEditorsButActive">
    <keyboard-shortcut first-keystroke="shift alt o" />
  </action>
  <action id="EditorDecreaseFontSize">
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="EditorIncreaseFontSize">
    <keyboard-shortcut first-keystroke="alt equals" />
  </action>
  <action id="EditorToggleUseSoftWraps">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="ctrl k" />
  </action>
  <action id="MarkExcludeRoot">
    <keyboard-shortcut first-keystroke="alt m" />
  </action>
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="down" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="right" />
  </action>
  <action id="Vcs.UpdateProject" />
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="alt semicolon" />
  </action>
</keymap>