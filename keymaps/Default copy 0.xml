<keymap version="1" name="Default copy 0" parent="$default">
  <action id="EditorDown">
    <keyboard-shortcut first-keystroke="down" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="EditorLeft">
    <keyboard-shortcut first-keystroke="left" />
    <keyboard-shortcut first-keystroke="alt m" />
  </action>
  <action id="EditorRight">
    <keyboard-shortcut first-keystroke="right" />
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="up" />
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
  <action id="NewDir">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="NewHtmlFile">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="NewJavaScriptFile">
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
  <action id="NewStylesheetFile">
    <keyboard-shortcut first-keystroke="alt s" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="ResetToMySettings">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="SelectNextOccurrence" />
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="down" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="right" />
  </action>
</keymap>